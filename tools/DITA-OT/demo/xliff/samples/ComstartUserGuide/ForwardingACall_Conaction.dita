<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "http://docs.oasis-open.org/dita/v1.1/OS/dtd/task.dtd">
<task xml:lang="en-us" id="forwarding">
    <title>Forwarding a call</title>
    <taskbody>
        <steps>            
            <step>
                <cmd>Press Program.</cmd>
            </step>
            <step>
                <cmd>Press Forward.</cmd>
            </step>
            <step>
                <cmd>Press an idle line button.</cmd>
            </step>
            <step id="dialoutside">
                <cmd>Dial the outside number.</cmd>
            </step>
            <step id="finalprogram">
                <cmd>Press Program.</cmd>
            </step>
        </steps>
    </taskbody>
</task>
