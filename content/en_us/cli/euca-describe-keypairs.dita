<reference outputclass="docs one-sidebar sidebar-first" id="euca-describe-keypairs">
	<title>euca-describe-keypairs</title>

	<shortdesc>Shows information about key pairs.</shortdesc>
  <refbody>
    <section>
    	<title>Syntax</title>
    	<codeblock>euca-describe-keypairs [ <b>keypair_name</b> ] [-v]</codeblock>
    </section>
  	<section>
  	
  		<title>Options</title>
  		<table>
  			<tgroup cols="3">
  				<colspec colwidth="25*"/>
  				<colspec colwidth="60*"/>
  				<colspec colwidth="15*"/>
  				<thead>
  					<row> 
  						<entry>Option</entry>
  						<entry>Description</entry> 
  						<entry>Required</entry>
  					</row>
  				</thead>
  				<tbody>
  					<row>
  						<entry><codeph>keypair_name</codeph></entry>
  						<entry>Key pair name to describe</entry>
  						<entry align="center">No</entry>
  					</row>
  					<row>
  						<entry><codeph>-v, --verbose</codeph></entry>
  						<entry>Displays all of the resources allocated by all users for the resource in question</entry>
  						<entry align="center">No</entry>

  					</row>
  				</tbody>
  			</tgroup>
  		</table>
  	</section>

  	
