<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task outputclass="docs one-sidebar sidebar-first" id="delete_snap">
    <title>Delete Snapshot</title>
    <shortdesc/>
    <taskbody>
        <context>
            <p>To delete a snapshot:</p>
        </context>
        <steps>
            <step>
                <cmd>Enter the following command:</cmd>
            	<info>
            		<codeblock>euca-delete-snapshot &lt;snapshot_id></codeblock>
            	</info>
            </step>
        </steps>
    	<example>
    	
    		<codeblock>euca-delete-snapshot snap-78a54011</codeblock>
    	</example>
    </taskbody>
</task>
